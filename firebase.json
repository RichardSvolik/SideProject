{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "index.html"
      }
    ]
  },
  "projects": {
    "default": "christmas-b1caa"
  },
  "emulators": {
    "firestore": {
      "port": 5001
    },
    "hosting": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
